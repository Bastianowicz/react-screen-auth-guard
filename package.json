{
  "name": "react-screen-auth-guard",
  "version": "0.1.0",
  "description": "Protect certain screens against unauthenticated (or authenticated) visits (e.g. member page, login, register, ...)",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "prepublishOnly": "npm run build"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": ""
  },
  "files": [
    "dist",
    "readme.md",
    "LICENSE"
  ],
  "types": "dist/index.d.ts",
  "keywords": [
    "js",
    "ts",
    "react",
    "react-native",
    "auth",
    "guard",
    "screen",
    "navigation"
  ],
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "rootDir": "./src",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(@react-native|react-native|@testing-library/react-native)/)"
    ]
  },
  "author": "Bastian Charlet",
  "license": "ISC",
  "dependencies": {
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@react-navigation/stack": ">=6",
    "react": ">=18"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^18.3.12",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.2.5"
  }
}
